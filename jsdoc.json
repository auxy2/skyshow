{
    "plugins": ["plugins/markdown", "plugins/summarize"],
    "recurseDepth": 10,
    "source": {
        "include": ["./src"],
        "includePattern": ".js, .env",
        "excludePattern": "(node_modules|docs)"
    },
    
    "opts": {
        "encoding": "utf8",
        "readme": "./readme.md",
        "destination": "docs/",
        "recurse": true,
        "verbose": true
    }
}